<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.myexample.ringtoneswap"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="842ca124acbf7b50e396e9fedd95705bfcd97f4b"/>

        <receiver
            android:name=".DigitsContactsUploadBroadcastReceiver"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.digits.sdk.android.UPLOAD_COMPLETE"/>
                <action android:name="com.digits.sdk.android.UPLOAD_FAILED"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>